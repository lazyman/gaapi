<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="special" extends="struts-default" namespace="/special">

		<!-- special list -->
		<action name="save*" method="save{1}" class="org.tingoo.ga.wwmis.action.SpecialAction">
			<result type="redirect">list</result>
			<result name="error">error.jsp</result>
		</action>
		<action name="*" method="{1}" class="org.tingoo.ga.wwmis.action.SpecialAction">
			<result>{1}.jsp</result>
		</action>

		<action name="revocation" method="revocation" class="org.tingoo.ga.wwmis.action.SpecialAction">
			<result type="redirect">list</result>
		</action>
	</package>
	

</struts>
