<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="case" extends="struts-default" namespace="/case">

		<action name="add" method="add" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>input.jsp</result>
			<result name="noauthority">noauthority.jsp</result>
		</action>
		<action name="modify" method="modify" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>input.jsp</result>
		</action>
		<action name="delete" method="delete" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result type="redirect">home</result>
			<result name="fail">/user/error.jsp</result>
		</action>
		<action name="save" method="save" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result type="redirect">home</result>
			<result name="error">error.jsp</result>
			<result name="pwdwrong">pwdwrong.jsp</result>
		</action>
		<action name="statistics" method="statistics" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result >statistics.jsp</result>
		</action>
		
		<action name="revocationlist" method="revocationlist" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>revocationlist.jsp</result>
			<result name="noauthority">noauthority.jsp</result>
		</action>
		<!-- evaluate,disposal,approval list -->
		<action name="*list" method="{1}list" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>list.jsp</result>
			<result name="noauthority">noauthority.jsp</result>
		</action>
		<!-- save evaluate,disposal,approval,revocation -->
		<action name="save*" method="save{1}" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result type="redirect">{1}list</result>
		</action>
		<!-- evaluate,disposal,approval,revocation -->
		<action name="evaluate" method="evaluate" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>evaluate.jsp</result>
		</action>
		<action name="disposal" method="evaluate" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>disposal.jsp</result>
		</action>
		<action name="approval" method="evaluate" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>approval.jsp</result>
		</action>
		<action name="revocation" method="revocation" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>revocation.jsp</result>
		</action>
		
		<action name="info" method="info" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>info.jsp</result>
		</action>
		<action name="input" method="input" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>input.jsp</result>
		</action>
		<action name="search" method="search" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>search.jsp</result>
		</action>
		<action name="report" method="report" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>report.jsp</result>
		</action>
		<action name="home" method="home" class="org.tingoo.ga.wwmis.action.CaseAction">
			<result>home.jsp</result>
		</action>
	</package>
	

</struts>
