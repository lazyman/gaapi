<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="trend" extends="struts-default" namespace="/trend">
		
		<action name="modify" method="modify" class="org.tingoo.ga.wwmis.action.TrendAction">
			<result>input.jsp</result>
		</action>
		<action name="delete" method="delete" class="org.tingoo.ga.wwmis.action.TrendAction">
			<result type="redirect">home</result>
			<result name="fail">/user/error.jsp</result>
		</action>
		<action name="save" method="save" class="org.tingoo.ga.wwmis.action.TrendAction">
			<result type="redirect">/special/list</result>
		</action>
		
	</package>
	
</struts>
